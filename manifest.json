{
    "manifest_version": 2,
    "name": "BrickHunter",
    "description": "Upload a WantedList from BrickLink and search automatic for offizial prices on the LEGO page.",
    "version": "0.0.6",
    "icons": {
        "96": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "https://www.lego.com/",
        "https://www.bricklink.com/"
    ]
}