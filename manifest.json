{
    "manifest_version": 2,
    "name": "BrickHunter",
    "description": "Upload a WantedList from BrickLink and search automatic for offizial prices on the LEGO page.",
    "version": "0.0.6",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icon_16.png",
            "24": "icon_24.png",
            "32": "icon_32.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "https://www.lego.com/",
        "https://www.bricklink.com/"
    ]
}