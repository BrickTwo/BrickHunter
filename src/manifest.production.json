{
    "manifest_version": 3,
    "name": "BrickHunter",
    "description": "BrickHunter",
    "version": "2.0.1",
    "background": {
        "service_worker": "/js/background.js"
    },
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_title": "BrickHunter"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}